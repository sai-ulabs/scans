{
  "name": "scans",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "sass": "sass --watch src/scss:src/css",
    "start": "browser-sync start --server --files \"src/**/*\" --startPath \"src/index.html\"",
    "builder": "browser-sync start --server --files \"src/**/*\" --startPath \"src/builder.html\"",
    "builder-hotel": "concurrently \"npm run sass\" \"npm run builder\" "
  },
  "devDependencies": {
    "browser-sync": "^2.24.5",
    "lowdb": "^1.0.0"
  },
  "dependencies": {
    "concurrently": "^3.6.0",
    "sass": "^1.9.2"
  }
}